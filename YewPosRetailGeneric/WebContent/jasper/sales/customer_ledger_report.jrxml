	<queryString>
		<![CDATA[select     x.customer_id
          ,x.customer_name
          ,x.customer_address
          ,x.customer_phone_no
          ,x.store_name
          ,x.address
          ,x.phone
          ,x.email
          ,x.currency_code

          ,y.inv_no
          ,y.inv_date
          ,y.inv_type
          ,y.dr_amount
          ,y.cr_amount
          ,y.balance_amount
          ,y.inv_status
          ,(y.balance_amount+y.dr_amount-y.cr_amount) final_amount
          -- ,y.customer_id
          -- ,y.display_order
from
(
    select c.id customer_id
          ,c.name customer_name
          ,concat(c.address,' : ',ifnull(c.city,'')) customer_address
          ,concat('Ph: ', c.phone_no) customer_phone_no
          ,s.name store_name
          ,s.address
          ,s.phone
          ,s.email
          ,cu.currency_code

    from in_m_customer c
    inner join gen_m_store s on c.store_id=s.id
    left join in_m_currency cu on s.currency_id=cu.id
    where(
          ( c.id=$P{W_customerID} and $P{W_customerID}<>0 )
          or
          ($P{W_customerID}=0)
         )
    and c.company_id = $P{W_companyID}
    and c.store_id = $P{W_storeID}
    and s.id=$P{W_storeID}

)x
left outer join
(
    select 'Initial Balance' inv_no
          ,null inv_date
          ,null inv_type
          ,0 dr_amount
          ,0 cr_amount
          ,case
              when $P{W_status}='A' then (select get_customer_op_balance(c.id,2,$P{W_companyID},$P{W_storeID},$P{W_finyrID},$P{W_startDate}))
              when $P{W_status}='P' then (select get_customer_op_balance(c.id,1,$P{W_companyID},$P{W_storeID},$P{W_finyrID},$P{W_startDate}))
              end balance_amount
          ,'' inv_status
          ,c.id customer_id
          ,1 display_order


    from in_m_customer c
    where(
          ( c.id=$P{W_customerID} and $P{W_customerID}<>0 )
          or
          ($P{W_customerID}=0)
         )
    and c.company_id=$P{W_companyID}
    and c.store_id=$P{W_storeID}

    union all

    select  'Opening Balance' inv_no
           ,p.as_on_date inv_date
           ,'OP' inv_type
           ,case
               when p.amount>=0 then p.amount
               else 0 end dr_amount
           ,case
               when p.amount < 0 then p.amount
               else 0 end cr_amount
           ,0 balance_amount
           ,'' inv_status
           ,p.customer_id
           ,2 display_order


    from in_t_customer_opening p
    where(
          ( p.customer_id=$P{W_customerID} and $P{W_customerID}<>0 )
          or
          ($P{W_customerID}=0)
         )
    and p.company_id=$P{W_companyID}
    and p.store_id=$P{W_storeID}
    and p.finyr_id=$P{W_finyrID}
    and p.amount <> 0
    and p.as_on_date>=$P{W_startDate} and p.as_on_date <= $P{W_endDate}
    and ( $P{W_status}='A' or $P{W_status} = 'P' )

    union all

    select p.inv_no
          ,p.inv_date
          ,'SAL' inv_type
          ,p.credit_amount dr_amount
          ,0 cr_amount
          ,0 balance_amount
          ,case
             when ifnull(p.is_posted,0)=1 then 'P'
             else 'U' end inv_status
          ,p.customer_id
          ,3 display_order


    from sal_t_sale p
    where(
          ( p.customer_id=$P{W_customerID} and $P{W_customerID}<>0 )
          or
          ($P{W_customerID}=0)
         )
    and p.customer_id<>0
    and (
          ( ifnull(p.is_posted,0)=1 and $P{W_status} = 'P' )
          or
          ($P{W_status}='A')
        )
    and p.inv_date>=$P{W_startDate} and p.inv_date <= $P{W_endDate}
    and p.credit_amount>0
    and p.company_id=$P{W_companyID}
    and p.store_id=$P{W_storeID}
    and p.finyr_id=$P{W_finyrID}

    union all

    select
           pr.inv_no
          ,pr.inv_date
          ,'SALRTN' inv_type
          ,0 dr_amount
          ,pr.net_amount cr_amount
          ,0 balance_amount
          ,case
             when ifnull(pr.is_posted,0)=1 then 'P'
             else 'U' end inv_status
          ,pr.customer_id
          ,3 display_order


    from sal_t_sale_return pr
    where(
          ( pr.customer_id=$P{W_customerID} and $P{W_customerID} <> 0 )
          or
          ($P{W_customerID} = 0)
         )
    and pr.customer_id<>0
    and (
          ( ifnull(pr.is_posted,0)=1 and $P{W_status} = 'P' )
          or
          ($P{W_status}='A')
        )
    and pr.inv_date>=$P{W_startDate} and pr.inv_date <= $P{W_endDate}
    and pr.company_id=$P{W_companyID}
    and pr.store_id=$P{W_storeID}
    and pr.finyr_id=$P{W_finyrID}

    union all

    select
           pr.inv_no
          ,pr.inv_date
          ,'PAY' inv_type
          ,0 dr_amount
          ,pr.pay_amount cr_amount
          ,0 balance_amount
          ,case
             when ifnull(pr.is_posted,0)=1 then 'P'
             else 'U' end inv_status
          ,pr.customer_id
          ,3 display_order


    from pay_t_customer pr
    where(
          ( pr.customer_id=$P{W_customerID} and $P{W_customerID} <> 0 )
          or
          ($P{W_customerID} = 0)
         )
    and pr.customer_id<>0


    and pr.inv_date>=$P{W_startDate} and pr.inv_date <= $P{W_endDate}
    and pr.company_id=$P{W_companyID}
    and pr.store_id=$P{W_storeID}
    and pr.finyr_id=$P{W_finyrID}


) y on x.customer_id=y.customer_id
order by x.customer_name,y.display_order,y.inv_date
;]]>
	</queryString>

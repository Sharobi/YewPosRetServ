            ,cast(a.special_disc_amount as char(8)) special_disc_amount_print
            ,cast(a.roundoff as char(8)) roundoff_print
            ,cast(a.gross_amount as char(8)) gross_amount_print
            ,cast(a.disc_amount as char(8)) disc_amount_print

            ,cast(a.retrun_amount as char(8))  retrun_amount
            ,cast(a.bill_amount as char(8)) bill_amount
            ,cast(a.total_tax_amount as char(8)) total_tax_amount


            ,a.cash_memo_header
            ,a.amount_lable
            ,a.reprint_leble

            ,a.CGST_lable
            ,a.CGST_AMT_lable
            ,a.SGST_lable
            ,a.SGST_AMT_lable
            ,a.schedule_name
			,a.remarks
			,a.item_remarks
			,a.user_name
			,a.user_code
			,a.salesman_name
 from
 (
   select x.item_id
            ,x.item_name
            ,x.batch_no
            ,x.expiry_date
            ,x.expiry_date_format
            ,x.pack_unit_id
            ,x.pack_unit_name
            ,x.pack_qty
            ,x.conversion
            ,x.loose_unit_id
            ,x.loose_qty
            ,x.mrp
            ,x.rate
            ,x.mrp_per_unit
            ,x.rate_per_unit
            ,x.disc_per
            ,x.disc
            ,x.tot_amount
            ,x.calculate_loose_qty



            ,x.tax_id
            ,x.tax_name
            ,x.tax_percentage
            ,x.tax_amount
            ,x.is_group_tax
            ,x.tax_mode

            ,x.SKU
            ,x.HSN_code
            ,x.purchase_cost_per_unit

            ,x.total_amount
            ,x.taxable_amount

            ,sum(x.CGST) CGST
            ,sum(x.CGST_percentage) CGST_percentage
            ,sum(x.SGST) SGST
            ,sum(x.SGST_percentage) SGST_percentage
            ,sum(x.IGST) IGST
            ,sum(x.IGST_percentage) IGST_percentage


,concat(cast(sum(x.CGST) as char(8)),'(',cast(cast(sum(x.CGST_percentage) as decimal(18,1)) as char(8)),'%)') CGST_print
            ,concat(cast(sum(x.SGST) as char(8)),'(',cast(cast(sum(x.SGST_percentage) as decimal(18,1)) as char(8)),'%)') SGST_print
            ,concat(cast(sum(x.IGST) as char(8)),'(',cast(cast(sum(x.IGST_percentage) as decimal(18,1)) as char(8)),'%)') IGST_print

            ,x.net_content
            ,x.group_id
            ,x.schedule_id
            ,x.manufacturer_id
            ,x.manufacturer_name
            ,x.manufacturer_code

            ,ifnull(x.customer_name,'') customer_name
            ,ifnull(x.customer_phone,'') customer_phone
            ,ifnull(x.customer_address,'')  customer_address
            ,ifnull(x.customer_pan,'')  customer_pan
            ,ifnull(x.customer_gst,'')  customer_gst
            ,ifnull(x.customer_dl,'') customer_dl
            ,ifnull(x.doctor_name,'') doctor_name

            ,ifnull(x.inv_no,'') inv_no
            ,x.inv_date
            ,x.inv_time
            ,x.due_date
            ,x.store_name
            ,x.store_address
			,x.store_post
			,x.store_state
			,x.store_country
            ,x.drug_license_no
            ,ifnull(x.GSTIN_No,'') GSTIN_No
            ,x.store_phone
			,x.store_email
            ,x.total_taxable_amount
            ,x.net_amount
            ,(select AmountToWords(x.net_amount) ) net_amount_words

            ,x.special_disc_amount
            ,x.roundoff
            ,x.gross_amount
            ,x.disc_amount


            ,0 retrun_amount
            ,x.net_amount bill_amount


            ,x.cash_memo_header
            ,x.amount_lable
            ,x.reprint_leble
            ,x.is_exclusive

            ,x.Tax_lable
            ,x.Taxable_lable
            ,x.CGST_lable
            ,x.CGST_AMT_lable
            ,x.SGST_lable
            ,x.SGST_AMT_lable
            ,x.schedule_name
            ,x.total_tax_amount
			,x.remarks
			,x.item_remarks
			,x.user_name
			,x.user_code
			,x.salesman_name
  from
  (
      select sd.item_id
            ,i.name item_name
            ,sd.batch_no
            ,sd.expiry_date
            ,convert(DATE_FORMAT(sd.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format

            ,sd.pack_unit_id
            ,u.code pack_unit_name
            ,sd.pack_qty
            ,sd.conversion
            ,i.loose_unit_id
            ,sd.loose_qty
            ,sd.mrp
            ,sd.rate
            ,0 mrp_per_unit
            ,sd.sale_rate rate_per_unit
            ,sd.disc_per
            ,sd.disc
            ,sd.net_amount tot_amount
            ,((sd.pack_qty*sd.conversion)+sd.loose_qty) calculate_loose_qty


            ,sd.tax_id
            ,t.name tax_name
            ,sd.tax_percentage
            ,sd.tax_amount
            ,sd.is_group_tax
            ,sd.tax_mode

            ,i.SKU
            ,i.HSN_code
            ,0 purchase_cost_per_unit


            ,(sd.gross_amount-ifnull(sd.disc,0)) taxable_amount
            ,(sd.gross_amount) total_amount
            --
            ,case
                   when tm.tax_mode='CGST' then (sd.tax_amount*tm.percentage/sd.tax_percentage)
                   else 0 end
            CGST
            ,case
                   when tm.tax_mode='CGST' then tm.percentage
                   else 0 end
            CGST_percentage
            ,case
                   when tm.tax_mode='SGST' then (sd.tax_amount*tm.percentage/sd.tax_percentage)
                   else 0 end
            SGST
            ,case
                   when tm.tax_mode='SGST' then tm.percentage
                   else 0 end
            SGST_percentage
            ,case
                   when tm.tax_mode='IGST' then (sd.tax_amount*tm.percentage/sd.tax_percentage)
                   else 0 end
            IGST
            ,case
                   when tm.tax_mode='IGST' then tm.percentage
                   else 0 end
            IGST_percentage

            ,i.net_content
            ,i.group_id
            ,i.schedule_id
            ,i.manufacturer_id
            ,m.name manufacturer_name
            ,substring(m.code,-3) manufacturer_code

            ,ifnull(s.customer_name,c.name) customer_name
            ,ifnull(s.customer_phone,c.phone_no) customer_phone
            ,ifnull(s.customer_address,c.address) customer_address
			,ifnull(c.pan_no,'') customer_pan
            ,ifnull(c.gst_no,'') customer_gst
            ,ifnull(c.dl_no,'') customer_dl
            ,ifnull(s.doctor_name,d.name) doctor_name

            ,s.inv_no
            ,s.inv_date
            ,'' inv_time
            ,s.due_date
            ,st.name store_name
			,st.address store_address
            ,st.postcode store_post
			,st.state store_state
			,st.country store_country
            ,concat('D.L No.:',st.dl_licence_no) drug_license_no
            ,st.tax_reg_no GSTIN_No
            ,st.phone store_phone
			,st.email store_email
            ,0 special_disc_amount
            ,s.gross_amount
            ,s.disc_amount
            ,s.net_amount
            ,(s.gross_amount-ifnull(s.disc_amount,0)) total_taxable_amount
            ,s.roundoff

            ,case
              when st.is_exclusive=1 then 'Sales Quotation'
              else 'Bill of Supply' end cash_memo_header
            ,case
              when st.is_exclusive=1 then 'Taxable Amt'
              else 'Amt**' end amount_lable
            ,case -- for isReprint
              when $P{W_IsReprint}=0 then ''
              else 'Duplicate Copy' end reprint_leble
             ,st.is_exclusive
            ,case
              when st.is_exclusive=1 then 'Tax%'
              else '' end Tax_lable
            ,case
              when st.is_exclusive=1 then 'Taxable'
              else '' end Taxable_lable
            ,case
              when st.is_exclusive=1 then 'CGST%'
              else '' end CGST_lable
            ,case
              when st.is_exclusive=1 then 'CGST AMT'
              else '' end CGST_AMT_lable
            ,case
              when st.is_exclusive=1 then 'SGST%'
              else '' end SGST_lable
            ,case
              when st.is_exclusive=1 then 'SGST AMT'
              else '' end SGST_AMT_lable

            ,concat(substring(sch.name,1,1),substring(replace(sch.name,' ',''),-2)) schedule_name
            ,s.tax_amount total_tax_amount
			,s.remarks
			,ifnull(sd.remarks,'') item_remarks
			,usr.user_name
			,usr.user_code
			,case when sd.salesman_id=0 then '' else sm.name end salesman_name
      from gen_m_store st
      inner join sal_t_sale_order s on st.id = s.store_id
      inner join sal_t_sale_order_details sd on s.id=sd.sale_order_id
      inner join in_m_item i on sd.item_id=i.id
      inner join in_m_unit u on i.pack_unit_id=u.id
      left outer join in_m_tax t on sd.tax_id=t.id
      left outer join in_m_tax_group_details tm on sd.tax_id=tm.parent_id
      left outer join in_m_manufacturer m on i.manufacturer_id=m.id
      left outer join in_m_schedule sch on i.schedule_id=sch.id
      left outer join in_m_customer c on s.customer_id=c.id
      left outer join in_m_doctor d on s.doctor_id=d.id
	  left outer join in_m_salesman sm on sd.salesman_id=sm.id
	  left outer join gen_m_user usr on s.created_by=usr.id
      where s.company_id = $P{W_CompanyID}
      and s.store_id = $P{W_StoreID}
      and st.id=$P{W_StoreID}
      and sd.sale_order_id=$P{W_SaleOrderID}

  )x
  group by   x.item_id
            ,x.item_name
            ,x.batch_no
            ,x.expiry_date
            ,x.expiry_date_format
            ,x.pack_unit_id
            ,x.pack_unit_name
            ,x.pack_qty
            ,x.conversion
            ,x.loose_unit_id
            ,x.loose_qty
            ,x.mrp
            ,x.rate
            ,x.mrp_per_unit
            ,x.rate_per_unit
            ,x.disc_per

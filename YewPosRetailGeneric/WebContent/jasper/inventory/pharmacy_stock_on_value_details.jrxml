                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) purchase_return_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) purchase_return_loose_qty
                      , sum(x.calculate_loose_qty) purchase_return_calculate_loose_qty
                      , (x.o_sale_rate) o_sale_rate
                      , (x.p_sale_rate) p_sale_rate

               from
               (
                      -- from purchase return
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(1),ifnull(a.loose_qty,0)*(1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(1) calculate_loose_qty
                             ,a.rate mrp
                             ,0 o_sale_rate
                             ,0 p_sale_rate
                      FROM pur_t_purchase_return_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date>=ifnull($P{W_FromDate},current_date())
                      and a.inv_date<=ifnull($P{W_ToDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )
               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion-- ,x.o_sale_rate,x.p_sale_rate


               union all

               -- from expiry
               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      ,0 pack_qty
                      ,0 loose_qty
                      ,0 calculate_loose_qty
                      ,0 sale_pack_qty
                      ,0 sale_loose_qty
                      ,0 sale_calculate_loose_qty
                      , 0 sale_return_pack_qty
                      , 0 sale_return_loose_qty
                      , 0 sale_return_calculate_loose_qty
                      , 0 purchase_pack_qty
                      , 0 purchase_loose_qty
                      , 0 purchase_calculate_loose_qty
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) purchase_return_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) purchase_return_loose_qty
                      , sum(x.calculate_loose_qty) purchase_return_calculate_loose_qty
                      , (x.o_sale_rate) o_sale_rate
                      , (x.p_sale_rate) p_sale_rate

               from
               (
                      -- from expiry


                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(-1),ifnull(a.loose_qty,0)*(-1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(-1) calculate_loose_qty
                             ,a.rate mrp
                             ,0 o_sale_rate
                             ,0 p_sale_rate

                      FROM exp_t_expiry_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date>=ifnull($P{W_FromDate},current_date())
                      and a.inv_date<=ifnull($P{W_ToDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )


               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion-- ,x.o_sale_rate,x.p_sale_rate

         )s


                group by
                 s.item_id
                ,s.batch_no
                ,s.expiry_date
                ,s.expiry_date_format
                ,s.pack_unit_id
                ,s.conversion

                #,s.o_sale_rate
                #,s.p_sale_rate


      )a

    )a

    )x
    inner join  in_m_item y on x.item_id=y.id
    inner join in_m_group g on y.group_id=g.id
    inner join gen_m_store s on s.id=$P{W_storeID}
    left join in_m_currency c on s.currency_id=c.id
    where s.id=$P{W_storeID}
    and   (
                  (y.group_id=$P{W_contentID} and ifnull($P{W_contentID},0) <> 0)
                  or
                  (ifnull($P{W_contentID},0)=0)
                )
   order by g.name,y.name ,x.expiry_date


   ;]]>

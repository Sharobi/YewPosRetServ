	<queryString>
		<![CDATA[select x.item_id
        ,y.name item_name
        ,g.name group_name
        ,x.batch_no
        ,x.expiry_date
        ,x.expiry_date_format
        ,x.conversion
        ,x.mrp
        ,x.pack_qty
        ,x.loose_qty
        ,x.calculate_loose_qty
        ,x.stock_amount
        ,x.stock_qty

        ,x.sale_amount
        ,x.sale_qty

        ,x.purchase_return_amount
        ,x.purchase_return_qty

        ,x.purchase_amount
        ,x.purchase_qty

        ,x.sale_return_amount
        ,x.sale_return_qty

        ,x.issue_amount
        ,x.issue_pack_qty
        ,x.issue_loose_qty
        ,x.issue_calculate_loose_qty
        ,x.issue_qty

        ,x.receipt_amount
        ,x.receipt_pack_qty
        ,x.receipt_loose_qty
        ,x.receipt_calculate_loose_qty
        ,x.receipt_qty

        ,x.closing_amount
        ,x.closing_pack_qty
        ,x.closing_loose_qty
        ,x.closing_calculate_loose_qty
        ,x.closing_qty

        ,s.name store_name
        ,s.address
        ,s.phone
        ,s.email
        ,c.currency_code

  from
  (
     select  a.item_id
                ,a.batch_no
                ,a.expiry_date
                ,a.expiry_date_format
                ,a.pack_unit_id
                ,a.conversion
                ,a.mrp
                ,a.pack_qty
                ,a.loose_qty
                ,a.calculate_loose_qty

                ,a.sale_pack_qty
                ,a.sale_loose_qty
                ,a.sale_calculate_loose_qty

                ,a.sale_return_pack_qty
                ,a.sale_return_loose_qty
                ,a.sale_return_calculate_loose_qty

                ,a.purchase_pack_qty
                ,a.purchase_loose_qty
                ,a.purchase_calculate_loose_qty

                ,a.purchase_return_pack_qty
                ,a.purchase_return_loose_qty
                ,a.purchase_return_calculate_loose_qty

                ,((a.pack_qty+(a.loose_qty/a.conversion))*a.mrp) stock_amount
                ,concat(a.pack_qty,'/',convert(a.loose_qty,UNSIGNED INTEGER) ,' [',convert(a.calculate_loose_qty,UNSIGNED INTEGER),'] ') stock_qty

                ,((a.sale_pack_qty+(a.sale_loose_qty/a.conversion))*a.mrp) sale_amount
                ,concat(a.sale_pack_qty,'/',convert(a.sale_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.sale_calculate_loose_qty,UNSIGNED INTEGER),'] ') sale_qty

                ,((a.purchase_return_pack_qty+(a.purchase_return_loose_qty/a.conversion))*a.mrp) purchase_return_amount
                ,concat(a.purchase_return_pack_qty,'/',convert(a.purchase_return_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.purchase_return_calculate_loose_qty,UNSIGNED INTEGER),'] ') purchase_return_qty

                ,((a.purchase_pack_qty+(a.purchase_loose_qty/a.conversion))*a.mrp) purchase_amount
                ,concat(a.purchase_pack_qty,'/',convert(a.purchase_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.purchase_calculate_loose_qty,UNSIGNED INTEGER),'] ') purchase_qty

                ,((a.sale_return_pack_qty+(a.sale_return_loose_qty/a.conversion))*a.mrp) sale_return_amount
                ,concat(a.sale_return_pack_qty,'/',convert(a.sale_return_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.sale_return_calculate_loose_qty,UNSIGNED INTEGER),'] ') sale_return_qty

                -- issue
                ,a.issue_pack_qty
                ,a.issue_loose_qty
                ,a.issue_calculate_loose_qty
                ,((a.issue_pack_qty+(a.issue_loose_qty/a.conversion))*a.mrp) issue_amount
                ,concat(a.issue_pack_qty,'/',convert(a.issue_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.issue_calculate_loose_qty,UNSIGNED INTEGER),'] ') issue_qty

                -- receipt
                ,a.receipt_pack_qty
                ,a.receipt_loose_qty
                ,a.receipt_calculate_loose_qty
                ,((a.receipt_pack_qty+(a.receipt_loose_qty/a.conversion))*a.mrp) receipt_amount
                ,concat(a.receipt_pack_qty,'/',convert(a.receipt_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.receipt_calculate_loose_qty,UNSIGNED INTEGER),'] ') receipt_qty

                -- closing
                ,a.closing_pack_qty
                ,a.closing_loose_qty
                ,a.closing_calculate_loose_qty
                ,((a.closing_pack_qty+(a.closing_loose_qty/a.conversion))*a.mrp) closing_amount
                ,concat(a.closing_pack_qty,'/',convert(a.closing_loose_qty,UNSIGNED INTEGER) ,' [',convert(a.closing_calculate_loose_qty,UNSIGNED INTEGER),'] ') closing_qty

       from

       (

         select  s.item_id
                ,s.batch_no
                ,s.expiry_date
                ,s.expiry_date_format
                ,s.pack_unit_id
                ,s.conversion
                ,max(s.mrp) mrp
                ,sum(s.pack_qty) pack_qty
                ,sum(s.loose_qty) loose_qty
                ,sum(s.calculate_loose_qty) calculate_loose_qty

                ,sum(s.sale_pack_qty) sale_pack_qty
                ,sum(s.sale_loose_qty) sale_loose_qty
                ,sum(s.sale_calculate_loose_qty) sale_calculate_loose_qty

                ,sum(s.purchase_return_pack_qty) purchase_return_pack_qty
                ,sum(s.purchase_return_loose_qty) purchase_return_loose_qty
                ,sum(s.purchase_return_calculate_loose_qty) purchase_return_calculate_loose_qty

                ,sum(s.purchase_pack_qty) purchase_pack_qty
                ,sum(s.purchase_loose_qty) purchase_loose_qty
                ,sum(s.purchase_calculate_loose_qty) purchase_calculate_loose_qty

                ,sum(s.sale_return_pack_qty) sale_return_pack_qty
                ,sum(s.sale_return_loose_qty) sale_return_loose_qty
                ,sum(s.sale_return_calculate_loose_qty) sale_return_calculate_loose_qty


                -- issue
                ,( sum(s.sale_calculate_loose_qty) + sum(s.purchase_return_calculate_loose_qty) ) issue_calculate_loose_qty
                ,FLOOR(( sum(s.sale_calculate_loose_qty) + sum(s.purchase_return_calculate_loose_qty) )/s.conversion) issue_pack_qty
                ,mod(( sum(s.sale_calculate_loose_qty) + sum(s.purchase_return_calculate_loose_qty) ),s.conversion) issue_loose_qty

                -- receipt
                ,( sum(s.purchase_calculate_loose_qty) + sum(s.sale_return_calculate_loose_qty) ) receipt_calculate_loose_qty
                ,FLOOR(( sum(s.purchase_calculate_loose_qty) + sum(s.sale_return_calculate_loose_qty) )/s.conversion) receipt_pack_qty
                ,mod(( sum(s.purchase_calculate_loose_qty) + sum(s.sale_return_calculate_loose_qty) ),s.conversion) receipt_loose_qty

                -- closing
                ,( sum(s.calculate_loose_qty)+ sum(s.purchase_calculate_loose_qty) + sum(s.sale_return_calculate_loose_qty) - sum(s.sale_calculate_loose_qty) - sum(s.purchase_return_calculate_loose_qty) ) closing_calculate_loose_qty
                ,FLOOR( ( sum(s.calculate_loose_qty)+ sum(s.purchase_calculate_loose_qty) + sum(s.sale_return_calculate_loose_qty) - sum(s.sale_calculate_loose_qty) - sum(s.purchase_return_calculate_loose_qty) ) /s.conversion) closing_pack_qty
                ,mod( ( sum(s.calculate_loose_qty)+ sum(s.purchase_calculate_loose_qty) + sum(s.sale_return_calculate_loose_qty) - sum(s.sale_calculate_loose_qty) - sum(s.purchase_return_calculate_loose_qty) ),s.conversion) closing_loose_qty

         from (


               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) loose_qty
                      , sum(x.calculate_loose_qty) calculate_loose_qty
                      , 0 sale_pack_qty
                      , 0 sale_loose_qty
                      , 0 sale_calculate_loose_qty
                      , 0 sale_return_pack_qty
                      , 0 sale_return_loose_qty
                      , 0 sale_return_calculate_loose_qty
                      , 0 purchase_pack_qty
                      , 0 purchase_loose_qty
                      , 0 purchase_calculate_loose_qty
                      , 0 purchase_return_pack_qty
                      , 0 purchase_return_loose_qty
                      , 0 purchase_return_calculate_loose_qty





               from
               (

                      -- from Stock
                      SELECT  a.item_id, a.batch_no, a.expiry_date, a.pack_unit_id, a.pack_qty, a.loose_qty,a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+ifnull(a.loose_qty,0) ) calculate_loose_qty
                             ,a.rate mrp
                      FROM inv_t_op_stock a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.as_on_date <=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )
                      union all


                      -- from sale
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(-1),ifnull(a.loose_qty,0)*(-1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(-1) calculate_loose_qty
                             ,a.rate mrp
                      FROM sal_t_sale_details a
                      inner join sal_t_sale s on a.sale_id=s.id
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and s.hold_flag=1
                      and a.inv_date<ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )



                      union all

                      -- from sale return
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, a.pack_qty,a.loose_qty, a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) ) calculate_loose_qty
                             ,a.rate mrp
                      FROM sal_t_sale_return_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date<ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )



                      union all

                      -- from purchase
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, a.pack_qty,a.loose_qty, a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) ) calculate_loose_qty
                             ,a.rate mrp
                      FROM pur_t_purchase_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date<ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )


                      union all
                      -- from purchase return
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(-1),ifnull(a.loose_qty,0)*(-1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(-1) calculate_loose_qty
                             ,a.rate mrp
                      FROM pur_t_purchase_return_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date<ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )

                      union all

                      -- from expiry issue
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(-1),ifnull(a.loose_qty,0)*(-1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(-1) calculate_loose_qty
                             ,a.rate mrp
                      FROM exp_t_expiry_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date<ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )





                      union all


                      -- from stock transfer in

                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, a.recvd_pack_qty,a.recvd_loose_qty, a.conversion
                             ,( ( ifnull(a.recvd_pack_qty,0)*a.conversion)+ifnull(a.recvd_loose_qty,0) ) calculate_loose_qty
                             ,a.rate mrp
                      FROM in_st_stock_transfer_details a
                      inner join in_st_stock_transfer b on b.id = a.stock_transfer_id
                      where a.company_id=$P{W_companyID}
                      and a.to_store_id=$P{W_storeID}
                      and a.to_finyr_id=$P{W_finyrID}
                      and b.receive_date <ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )




               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion -- x.mrp
               having sum(x.calculate_loose_qty) >0


               union all

               --  from sale
               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      ,0 pack_qty
                      ,0 loose_qty
                      ,0 calculate_loose_qty
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) sale_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) sale_loose_qty
                      , sum(x.calculate_loose_qty) sale_calculate_loose_qty
                      , 0 sale_return_pack_qty
                      , 0 sale_return_loose_qty
                      , 0 sale_return_calculate_loose_qty
                      , 0 purchase_pack_qty
                      , 0 purchase_loose_qty
                      , 0 purchase_calculate_loose_qty
                      , 0 purchase_return_pack_qty
                      , 0 purchase_return_loose_qty
                      , 0 purchase_return_calculate_loose_qty






               from
               (
                      -- from sale
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(1),ifnull(a.loose_qty,0)*(1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(1) calculate_loose_qty
                             ,a.rate mrp
                      FROM sal_t_sale_details a
                      inner join sal_t_sale s on a.sale_id=s.id
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and s.hold_flag=1
                      and a.inv_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )
               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion -- ,x.mrp


               union all

               -- from sale return
               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      ,0 pack_qty
                      ,0 loose_qty
                      ,0 calculate_loose_qty
                      ,0 sale_pack_qty
                      ,0 sale_loose_qty
                      ,0 sale_calculate_loose_qty
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) sale_return_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) sale_return_loose_qty
                      , sum(x.calculate_loose_qty) sale_return_calculate_loose_qty
                      , 0 purchase_pack_qty
                      , 0 purchase_loose_qty
                      , 0 purchase_calculate_loose_qty
                      , 0 purchase_return_pack_qty
                      , 0 purchase_return_loose_qty
                      , 0 purchase_return_calculate_loose_qty





               from
               (
                      -- from sale return
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, a.pack_qty,a.loose_qty, a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) ) calculate_loose_qty
                             ,a.rate mrp
                      FROM sal_t_sale_return_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )


	       /* ###    Added For STOCK TRANSFER IN-OUT ;  2020-02-13 Onwards  ### */


					 union all



               -- stock transfer in  considered as added to sale return

                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.recvd_pack_qty,0)*(1),ifnull(a.recvd_loose_qty,0)*(1)
					         , a.conversion
                             ,( ( ifnull(a.recvd_pack_qty,0)*a.conversion)+ifnull(a.recvd_loose_qty,0) )*(1) calculate_loose_qty
                             ,a.rate mrp
                      FROM in_st_stock_transfer_details a
					  inner join in_st_stock_transfer b on b.id = a.stock_transfer_id
                      where a.company_id=$P{W_companyID}
                      and a.to_store_id=$P{W_storeID}
                      and a.to_finyr_id=$P{W_finyrID}
                      and b.receive_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )




               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion -- ,x.mrp


               union all

               -- from purchase
               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      ,0 pack_qty
                      ,0 loose_qty
                      ,0 calculate_loose_qty
                      ,0 sale_pack_qty
                      ,0 sale_loose_qty
                      ,0 sale_calculate_loose_qty
                      , 0 sale_return_pack_qty
                      , 0 sale_return_loose_qty
                      , 0 sale_return_calculate_loose_qty
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) purchase_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) purchase_loose_qty
                      , sum(x.calculate_loose_qty) purchase_calculate_loose_qty
                      , 0 purchase_return_pack_qty
                      , 0 purchase_return_loose_qty
                      , 0 purchase_return_calculate_loose_qty




               from
               (
                      -- from purchase
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, a.pack_qty,a.loose_qty, a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) ) calculate_loose_qty
                             ,a.rate mrp
                      FROM pur_t_purchase_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )

               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion -- ,x.mrp


               union all

               -- from purchase return
               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      ,0 pack_qty
                      ,0 loose_qty
                      ,0 calculate_loose_qty
                      ,0 sale_pack_qty
                      ,0 sale_loose_qty
                      ,0 sale_calculate_loose_qty
                      , 0 sale_return_pack_qty
                      , 0 sale_return_loose_qty
                      , 0 sale_return_calculate_loose_qty
                      , 0 purchase_pack_qty
                      , 0 purchase_loose_qty
                      , 0 purchase_calculate_loose_qty
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) purchase_return_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) purchase_return_loose_qty
                      , sum(x.calculate_loose_qty) purchase_return_calculate_loose_qty





               from
               (
                      -- from purchase return
                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(1),ifnull(a.loose_qty,0)*(1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(1) calculate_loose_qty
                             ,a.rate mrp
                      FROM pur_t_purchase_return_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )



	       /* ###    Added For STOCK TRANSFER IN-OUT ;  2020-02-13 Onwards  ### */


					 union all


               -- stock transfer out  considered as added to purchase return

                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.act_send_pack_qty,0)*(1),ifnull(a.act_send_loose_qty,0)*(1), a.conversion
                             ,( ( ifnull(a.act_send_pack_qty,0)*a.conversion)+ifnull(a.act_send_loose_qty,0) )*(1) calculate_loose_qty
                             ,a.rate mrp
                      FROM in_st_stock_transfer_details a
                      where a.company_id=$P{W_companyID}
                      and a.from_store_id=$P{W_storeID}
                      and a.from_finyr_id=$P{W_finyrID}
                      and a.stock_transfer_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )




               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion  -- ,x.mrp


               union all

               -- from expiry
               select x.item_id
                      , x.batch_no
                      , x.expiry_date
                      , convert(DATE_FORMAT(x.expiry_date, '%m/%y'),CHAR CHARACTER SET utf8) expiry_date_format
                      , x.pack_unit_id
                      , x.conversion
                      , max(x.mrp) mrp
                      ,0 pack_qty
                      ,0 loose_qty
                      ,0 calculate_loose_qty
                      ,0 sale_pack_qty
                      ,0 sale_loose_qty
                      ,0 sale_calculate_loose_qty
                      , 0 sale_return_pack_qty
                      , 0 sale_return_loose_qty
                      , 0 sale_return_calculate_loose_qty
                      , 0 purchase_pack_qty
                      , 0 purchase_loose_qty
                      , 0 purchase_calculate_loose_qty
                      , FLOOR(sum(x.calculate_loose_qty)/x.conversion) purchase_return_pack_qty
                      , mod(sum(x.calculate_loose_qty),x.conversion) purchase_return_loose_qty
                      , sum(x.calculate_loose_qty) purchase_return_calculate_loose_qty






               from
               (
                      -- from expiry


                      SELECT a.item_id,a.batch_no, a.expiry_date, a.pack_unit_id, ifnull(a.pack_qty,0)*(-1),ifnull(a.loose_qty,0)*(-1), a.conversion
                             ,( ( ifnull(a.pack_qty,0)*a.conversion)+( FLOOR(ifnull(a.free_qty,0))*a.conversion)+ifnull(a.loose_qty,0) )*(-1) calculate_loose_qty
                             ,a.rate mrp

                      FROM exp_t_expiry_details a
                      where a.company_id=$P{W_companyID}
                      and a.store_id=$P{W_storeID}
                      and a.finyr_id=$P{W_finyrID}
                      and a.inv_date=ifnull($P{W_asOnDate},current_date())
                      and (
                           (a.item_id=$P{W_itemID} and ifnull($P{W_itemID},0)<>0)
                           or
                           (ifnull($P{W_itemID},0)=0)
                          )



               )x
               group by x.item_id, x.batch_no, x.expiry_date, x.pack_unit_id,x.conversion  -- ,x.mrp

         )s


                group by
                 s.item_id
                ,s.batch_no
                ,s.expiry_date
                ,s.expiry_date_format
                ,s.pack_unit_id
                ,s.conversion


      )a


    )x
    inner join  in_m_item y on x.item_id=y.id
    inner join in_m_group g on y.group_id=g.id
    inner join gen_m_store s on s.id=$P{W_storeID}
    left join in_m_currency c on s.currency_id=c.id
    where s.id=$P{W_storeID}
   order by g.name,y.name ,x.expiry_date


   ;]]>

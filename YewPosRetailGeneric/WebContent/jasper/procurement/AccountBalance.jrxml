	<queryString>
		<![CDATA[select
              s.name store_name
              ,s.address
              ,s.phone
              ,s.email
              ,cu.currency_code

              ,x.type_id
              ,x.type_name
              ,x.account_id
              ,x.account_name
              ,x.group_id
              ,x.group_name
              ,x.row_current_balance
              ,x.current_balance
              ,x.balance_type
   from
   (
        select
               x.type_id
              ,x.type_name
              ,x.account_id
              ,x.account_name
              ,x.group_id
              ,x.group_name
              ,x.currentBalance row_current_balance
              ,abs(x.currentBalance) current_balance
              ,case
                  when x.currentBalance >=0 then 'Dr'
                  else 'Cr' end balance_type
        from
        (
            select t.id type_id
                  ,t.type_name
                  ,a.id account_id
                  ,a.name account_name
                  ,g.id group_id
                  ,g.name  group_name
                  ,( select get_ledgerBalance_asOn( a.id,a.group_id,2,$P{W_companyID},$P{W_storeID},$P{W_finyrID},$P{W_asOnDate} ) ) currentBalance

            from acc_m_account a
            inner join acc_m_group g on a.group_id=g.id
            inner join para_account_type t on g.account_type_id=t.id
            where(
                  ( a.id= $P{W_ledgerID} and ifnull($P{W_ledgerID},0) <> 0 )
                  or
                  ( ifnull($P{W_ledgerID},0)=0)
                 )
            and (
                  ( a.group_id =$P{W_groupID} and ifnull($P{W_groupID},0) <> 0 )
                  or
                  ( ifnull($P{W_groupID},0)=0)
                )
            and a.company_id=$P{W_companyID}
            and a.store_id=$P{W_storeID}
        )x

  )x
  inner join gen_m_store s on s.id=$P{W_storeID}
  left join in_m_currency cu on s.currency_id=cu.id
  ;]]>
	</queryString>

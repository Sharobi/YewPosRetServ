	<parameter name="W_endDate" class="java.util.Date"/>
	<parameter name="W_type" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.inv_no
      ,a.inv_date
      ,a.item_name
      ,a.tax_name
      ,a.tax_percentage
      ,a.inv_type
      ,a.accessible_amount
      ,a.tax_amount
      ,a.CGST
      ,a.SGST
      ,a.VAT

      ,case
        when $P{W_type}='S' then 'Sale Tax Integrity Report From'
        when $P{W_type}='P' then 'Purchase Tax Integrity Report From'
        else 'Tax Integrity Report From' end report_header
      ,s.name store_name
      ,s.address
      ,s.phone
      ,s.email
      ,c.currency_code
from
(
   select x.inv_no
          ,x.inv_date
          ,x.item_name
          ,x.tax_name
          ,x.tax_percentage
          ,x.inv_type
          ,sum(x.accessible_amount) accessible_amount
          ,sum(x.tax_amount) tax_amount
          ,sum(x.CGST) CGST
          ,sum(x.SGST) SGST
          ,sum(x.VAT) VAT
   from
   (
        select x.inv_no
              ,x.inv_date
              ,x.item_name
              ,x.tax_name
              ,x.tax_percentage
              ,x.inv_type
              ,x.accessible_amount
              ,x.tax_amount
              ,(x.CGST) CGST
              ,(x.SGST) SGST
              ,(x.VAT) VAT
        from
        (
            select  s.inv_no
                   ,s.inv_date
                   ,i.name item_name
                   ,sd.batch_no
                   ,sd.expiry_date
                   ,t.name tax_name
                   ,sd.tax_percentage
                   ,case
                       when s.credit_amount>0 then 'Debit Sale'
                       else 'Cash Sale' end inv_type

                   ,(sd.amount-sd.disc) accessible_amount
                   ,sd.tax_amount
                   ,((sd.tax_amount*100)/t.tax_percentage)*(t.CGST_percentage/100) CGST
                   ,((sd.tax_amount*100)/t.tax_percentage)*(t.SGST_percentage/100) SGST
                   ,((sd.tax_amount*100)/t.tax_percentage)*(t.IGST_percentage/100) VAT



            from sal_t_sale s
            inner join sal_t_sale_details sd on s.id=sd.sale_id
            inner join in_m_item i on sd.item_id=i.id
            inner join in_m_tax t on sd.tax_id=t.id
            left outer join
            (
              select x.tax_id,x.tax_percentage,sum(x.CGST_percentage) CGST_percentage,sum(x.SGST_percentage) SGST_percentage,sum(x.IGST_percentage) IGST_percentage
              from
              (
                  select  t.id tax_id
                         ,t.percentage tax_percentage
                         ,case
                            when tg.tax_mode='CGST' then tg.percentage
                            else 0 end CGST_percentage
                         ,case
                            when tg.tax_mode='SGST' then tg.percentage
                            else 0 end SGST_percentage
                         ,case
                            when tg.tax_mode='IGST' then tg.percentage
                            else 0 end IGST_percentage

                  from in_m_tax t
                  left outer join in_m_tax_group_details tg on t.id=tg.parent_id
              ) x group by x.tax_id,x.tax_percentage
            ) t on sd.tax_id=t.tax_id

            where s.company_id = $P{W_companyID}
            and s.store_id = $P{W_storeID}
            and s.finyr_id=$P{W_finyrID}
            and s.inv_date between $P{W_startDate} and $P{W_endDate}
            and $P{W_type}='S'
            and s.is_posted=1

        )x


        union all

        select x.inv_no
              ,x.inv_date
              ,x.item_name
              ,x.tax_name
              ,x.tax_percentage
              ,x.inv_type
              ,x.accessible_amount
              ,x.tax_amount
              ,(x.CGST) CGST
              ,(x.SGST) SGST
              ,(x.VAT) VAT
        from
        (
            select  s.inv_no
                   ,s.inv_date
                   ,i.name item_name
                   ,sd.batch_no
                   ,sd.expiry_date
                   ,t.name tax_name
                   ,sd.tax_percentage
                   ,'Sale Return' inv_type

                   ,(sd.amount-sd.disc) accessible_amount
                   ,(0-sd.tax_amount) tax_amount
                   ,((sd.tax_amount*100)/t.tax_percentage)*(t.CGST_percentage/100) CGST
                   ,((sd.tax_amount*100)/t.tax_percentage)*(t.SGST_percentage/100) SGST
                   ,((sd.tax_amount*100)/t.tax_percentage)*(t.IGST_percentage/100) VAT


            from sal_t_sale_return s
            inner join sal_t_sale_return_details sd on s.id=sd.sale_return_id
            inner join in_m_item i on sd.item_id=i.id
            inner join in_m_tax t on sd.tax_id=t.id
            left outer join
            (
              select x.tax_id,x.tax_percentage,sum(x.CGST_percentage) CGST_percentage,sum(x.SGST_percentage) SGST_percentage,sum(x.IGST_percentage) IGST_percentage
              from
              (
                  select  t.id tax_id
                         ,t.percentage tax_percentage
                         ,case
                            when tg.tax_mode='CGST' then tg.percentage
                            else 0 end CGST_percentage
                         ,case
                            when tg.tax_mode='SGST' then tg.percentage
                            else 0 end SGST_percentage
                         ,case
                            when tg.tax_mode='IGST' then tg.percentage
                            else 0 end IGST_percentage

                  from in_m_tax t
                  left outer join in_m_tax_group_details tg on t.id=tg.parent_id
              ) x group by x.tax_id,x.tax_percentage
            ) t on sd.tax_id=t.tax_id


            where s.company_id = $P{W_companyID}
            and s.store_id = $P{W_storeID}
            and s.finyr_id=$P{W_finyrID}
            and s.inv_date between $P{W_startDate} and $P{W_endDate}
            and $P{W_type}='S'
            and s.is_posted=1

        )x


        union all

        select x.inv_no
              ,x.inv_date
              ,x.item_name
              ,x.tax_name
              ,x.tax_percentage
              ,x.inv_type
              ,x.accessible_amount
              ,x.tax_amount
              ,(x.CGST) CGST
              ,(x.SGST) SGST
              ,(x.VAT) VAT
        from
        (
            select  p.inv_no
                   ,p.inv_date
                   ,i.name item_name
                   ,pd.batch_no
                   ,pd.expiry_date
                   ,t.name tax_name
                   ,pd.tax_percentage
                   ,pm.mode inv_type
                   ,(pd.purchase_net_amount-pd.tax_amount) accessible_amount
                   ,pd.tax_amount
                   ,((pd.tax_amount*100)/t.tax_percentage)*(t.CGST_percentage/100) CGST
                   ,((pd.tax_amount*100)/t.tax_percentage)*(t.SGST_percentage/100) SGST
                   ,((pd.tax_amount*100)/t.tax_percentage)*(t.IGST_percentage/100) VAT


            from pur_t_purchase p
            inner join pur_t_purchase_details pd on p.id=pd.purchase_id
            inner join com_payment_mode pm on p.inv_mode=pm.id
            inner join in_m_item i on pd.item_id=i.id
            inner join in_m_tax t on pd.tax_id=t.id
            left outer join
            (
              select x.tax_id,x.tax_percentage,sum(x.CGST_percentage) CGST_percentage,sum(x.SGST_percentage) SGST_percentage,sum(x.IGST_percentage) IGST_percentage
              from
              (
                  select  t.id tax_id
                         ,t.percentage tax_percentage
                         ,case
                            when tg.tax_mode='CGST' then tg.percentage
                            else 0 end CGST_percentage
                         ,case
                            when tg.tax_mode='SGST' then tg.percentage
                            else 0 end SGST_percentage
                         ,case
                            when tg.tax_mode='IGST' then tg.percentage
                            else 0 end IGST_percentage

                  from in_m_tax t
                  left outer join in_m_tax_group_details tg on t.id=tg.parent_id
              ) x group by x.tax_id,x.tax_percentage
            ) t on pd.tax_id=t.tax_id

            where p.company_id = $P{W_companyID}
            and p.store_id = $P{W_storeID}
            and p.finyr_id=$P{W_finyrID}
            and p.inv_date between $P{W_startDate} and $P{W_endDate}
            and $P{W_type}='P'
            and p.is_posted=1

        )x


        union all

        select x.inv_no
              ,x.inv_date
              ,x.item_name
              ,x.tax_name
              ,x.tax_percentage
              ,x.inv_type
              ,x.accessible_amount
              ,x.tax_amount
              ,(x.CGST) CGST
              ,(x.SGST) SGST
              ,(x.VAT) VAT
        from
        (
            select  p.inv_no
                   ,p.inv_date
                   ,i.name item_name
                   ,pd.batch_no
                   ,pd.expiry_date
                   ,t.name tax_name
                   ,pd.tax_percentage
                   ,'Purchase Return' inv_type
                   ,((pd.pack_qty*pd.rate)-pd.disc) accessible_amount
                   ,(0-pd.tax_amount) tax_amount
                   ,((pd.tax_amount*100)/t.tax_percentage)*(t.CGST_percentage/100) CGST
                   ,((pd.tax_amount*100)/t.tax_percentage)*(t.SGST_percentage/100) SGST
                   ,((pd.tax_amount*100)/t.tax_percentage)*(t.IGST_percentage/100) VAT




            from pur_t_purchase_return p
            inner join pur_t_purchase_return_details pd on p.id=pd.purchase_return_id
            inner join in_m_item i on pd.item_id=i.id
            inner join in_m_tax t on pd.tax_id=t.id
            left join com_payment_mode pm on p.inv_mode=pm.id
            left outer join
            (
              select x.tax_id,x.tax_percentage,sum(x.CGST_percentage) CGST_percentage,sum(x.SGST_percentage) SGST_percentage,sum(x.IGST_percentage) IGST_percentage
              from
              (
                  select  t.id tax_id
                         ,t.percentage tax_percentage
                         ,case
                            when tg.tax_mode='CGST' then tg.percentage
                            else 0 end CGST_percentage
                         ,case
                            when tg.tax_mode='SGST' then tg.percentage
                            else 0 end SGST_percentage
                         ,case
                            when tg.tax_mode='IGST' then tg.percentage
                            else 0 end IGST_percentage

                  from in_m_tax t
                  left outer join in_m_tax_group_details tg on t.id=tg.parent_id
              ) x group by x.tax_id,x.tax_percentage
            ) t on pd.tax_id=t.tax_id

            where p.company_id = $P{W_companyID}
            and p.store_id = $P{W_storeID}
            and p.finyr_id=$P{W_finyrID}
            and p.inv_date between $P{W_startDate} and $P{W_endDate}
            and $P{W_type}='P'
            and p.is_posted=1


        )x

  )x
  group by x.inv_no
          ,x.inv_date
          ,x.item_name
          ,x.tax_name
          ,x.tax_percentage
          ,x.inv_type


)a
inner join gen_m_store s on s.id=$P{W_storeID}
left join in_m_currency c on s.currency_id=c.id

order by a.inv_type,a.inv_date desc ,a.inv_no
;]]>
	</queryString>

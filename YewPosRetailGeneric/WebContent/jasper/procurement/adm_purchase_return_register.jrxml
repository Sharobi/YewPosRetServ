	<queryString>
		<![CDATA[select x.purchase_return_id
		  ,x.store_id
          ,x.inv_no
          ,x.inv_date
          ,x.inv_mode_name
	        ,x.remarks
          ,x.distributor_name

          ,x.gross_amount
          ,x.ed_amount
          ,x.disc_amount
          #,x.vat_amount
          #,x.tax_amount
          ,x.tax_amount vat_amount
          ,x.adj_amount
          ,x.roundoff
          ,concat('(',x.roundoff,')') roundoffText
          ,x.net_amount
          ,x.total_mrp

          ,x.item_name
          ,x.item_id
          ,x.expiry_date
          ,x.batch_no
          ,x.mrp
          ,x.rate
          ,x.amount
          ,x.pack_qty
          ,x.conversion
          ,x.loose_qty
          ,x.free_qty
          ,x.ed_per
          ,x.ed

          ,x.tax_per vat_per
          ,x.tax vat

          ,x.disc_per
          ,x.disc
          ,concat(x.pack_qty,'/',x.loose_qty ,' [',convert((x.pack_qty*x.conversion)+x.loose_qty,UNSIGNED INTEGER),'] ') stock



          ,s.name store_name
          ,s.address
          ,s.phone
          ,s.email
          ,c.currency_code

          ,x.purchase_inv_no

  from
  (
       select p.id purchase_return_id
          ,p.inv_no
          ,p.inv_date
          ,p.inv_mode
          ,ifnull(pm.mode,'') inv_mode_name
          ,p.gross_amount
          ,p.ed_amount
          ,p.disc_amount
          ,p.vat_amount
          ,p.tax_amount
          ,p.adj_amount
          ,p.roundoff
          ,p.net_amount
          ,ifnull(p.total_mrp,0) total_mrp
          ,p.distributor_id
          ,d.name distributor_name
          ,p.remarks
          ,p.is_posted
          ,length(trim(d.registration_no)) len_registration_no

          ,i.name item_name
          ,pd.item_id
          ,pd.expiry_date
          ,pd.batch_no
          ,pd.mrp
          ,pd.rate
          ,pd.amount
          ,pd.pack_qty
          ,pd.conversion
          ,pd.loose_qty
          ,pd.free_qty
          ,pd.ed_per
          ,pd.ed
          ,pd.tax_percentage tax_per
          ,pd.tax_amount tax

          ,pd.disc_per
          ,pd.disc

          ,pd.purchase_inv_no
		  ,p.store_id


      from pur_t_purchase_return p
      inner join in_m_distributor d on p.distributor_id=d.id
      inner join pur_t_purchase_return_details pd on p.id=pd.purchase_return_id
      inner join in_m_item i on pd.item_id=i.id
      left outer join com_payment_mode pm on p.inv_mode=pm.id
      where p.company_id=$P{W_companyID}
      and p.is_deleted=0
      and p.is_posted=1
      and p.inv_date between $P{W_startDate} and $P{W_endDate}
      and (
            ( p.distributor_id=$P{W_distributorID} and ifnull($P{W_distributorID},0) <> 0   )

            or
            ( ifnull($P{W_distributorID},0)=0  )

          )
  )x
  inner join gen_m_store s on s.id=x.store_id
  left join in_m_currency c on s.currency_id=c.id
  order by x.store_id,x.inv_date desc,x.inv_no

;]]>
	</queryString>
